{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
  "comma-dangle": ["error", "never"],
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  "no-underscore-dangle": "off",
  }
}
